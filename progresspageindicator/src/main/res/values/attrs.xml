<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleIndicator">
        <attr name="circle_size" format="dimension" />
        <attr name="inner_stroke_size" format="dimension" />
        <attr name="outer_stroke_size" format="dimension" />

        <attr name="visited_color" format="color" />
        <attr name="unvisited_color" format="color" />
        <attr name="active_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressIndicator">
        <attr name="viewpager" format="reference" />
        <attr name="indicator_margin" format="dimension" />
    </declare-styleable>
</resources>
